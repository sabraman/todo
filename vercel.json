{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"packageManager": "pnpm"
			}
		}
	],
	"routes": [
		{
			"src": "/assets/(.*)",
			"dest": "/assets/$1",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			}
		},
		{
			"src": "/(.*)\\.(?:js|jsx|ts|tsx)$",
			"headers": {
				"content-type": "application/javascript"
			},
			"dest": "/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	]
}
